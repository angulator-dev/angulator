{
  "$schema": "http://json-schema.org/schema",
  "id": "Command",
  "title": "Command Schematic",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "The name of the command.",
      "x-prompt": "What name would you like to use for the command?",
      "$default": {
        "$source": "argv",
        "index": 0
      }
    }
  },
  "required": [
    "name"
  ]
}
